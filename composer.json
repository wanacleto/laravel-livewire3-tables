{
    "name": "wanacleto/laravel-livewire3-tables",
    "description": "A dynamic, responsive Laravel Livewire 3 table component with searching, sorting, checkboxes, and pagination.",
    "homepage": "https://github.com/wanacleto/laravel-livewire3-tables",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Dion",
            "email": "kdion4891@icloud.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Wanacleto\\LaravelLivewireTables\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wanacleto\\LaravelLivewireTables\\Providers\\TableServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.0",
        "livewire/livewire": "^3.0",
        "laravel/framework": "^9.0|^10.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
